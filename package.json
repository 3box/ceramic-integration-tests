{
  "name": "ceramic-integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for the Ceramic Network",
  "author": "Spencer T Brody <spencer@3box.io>",
  "license": "(APACHE-2.0 OR MIT)",
  "bugs": {
    "url": "https://github.com/3box/ceramic-integration-tests/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3box/ceramic-integration-tests.git"
  },
  "keywords": [
    "ceramic",
    "3box",
    "testing"
  ],
  "homepage": "https://github.com/3box/ceramic-integration-tests#readme",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "node_modules/.bin/tsc -p tsconfig.json",
    "launch:services": "node build/serviceLauncher",
    "prebuild": "npm run clean",
    "postinstall": "node node_modules/node-config-ts/bin/cli",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest --config jest.config.json --runInBand --verbose",
    "test:ci": "node --experimental-vm-modules ./node_modules/.bin/jest --config jest.config.ci.json --runInBand --verbose --forceExit",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "@3id/did-provider": "^0.4.1",
    "@aws-sdk/client-api-gateway": "^3.150.0",
    "@aws-sdk/client-ecs": "^3.150.0",
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@ceramicnetwork/3id-did-resolver": "^2.1.6",
    "@ceramicnetwork/cli": "^2.6.0",
    "@ceramicnetwork/common": "^2.5.0",
    "@ceramicnetwork/core": "^2.8.0",
    "@ceramicnetwork/http-client": "^2.3.3",
    "@ceramicnetwork/ipfs-daemon": "^2.1.3",
    "@ceramicnetwork/stream-tile": "^2.4.2",
    "@ceramicnetwork/streamid": "^2.3.2",
    "@jest/reporters": "^28.1.3",
    "@types/jest": "^28.1.7",
    "babel-jest": "^28.1.3",
    "did-jwt": "^6.6.0",
    "dids": "^3.3.0",
    "ipfs-http-client": "^57.0.3",
    "jest": "^28.1.3",
    "jest-resolver-enhanced": "^1.0.1",
    "key-did-provider-ed25519": "^2.0.1",
    "key-did-resolver": "^2.1.0",
    "node-config-ts": "^3.1.0",
    "node-jq": "^2.3.3",
    "tmp-promise": "^3.0.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "typestub-multihashes": "^0.0.4",
    "unique-names-generator": "^4.7.1"
  }
}
