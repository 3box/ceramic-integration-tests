{
  "name": "ceramic-integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for the Ceramic Network",
  "author": "Spencer T Brody <spencer@3box.io>",
  "license": "(APACHE-2.0 OR MIT)",
  "bugs": {
    "url": "https://github.com/3box/ceramic-integration-tests/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3box/ceramic-integration-tests.git"
  },
  "keywords": [
    "ceramic",
    "3box",
    "testing"
  ],
  "homepage": "https://github.com/3box/ceramic-integration-tests#readme",
  "scripts": {
    "build": "node_modules/.bin/tsc -p tsconfig.json",
    "launch:services": "node build/serviceLauncher",
    "prebuild": "npm run clean",
    "postinstall": "node-config-ts",
    "test": "./node_modules/.bin/jest --runInBand",
    "test:ci": "./node_modules/.bin/jest --runInBand --forceExit",
    "clean": "rm -rf ./build"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@ceramicnetwork/cli": "^0.21.0-rc.6",
    "@ceramicnetwork/common": "^0.16.0-rc.6",
    "@ceramicnetwork/core": "^0.20.0-rc.6",
    "@ceramicnetwork/docid": "^0.5.0-rc.3",
    "@ceramicnetwork/http-client": "^0.10.0-rc.6",
    "@jest/reporters": "^26.6.2",
    "@types/jest": "^26.0.19",
    "babel-jest": "^26.6.3",
    "dag-jose": "^0.3.0",
    "ipfs-core": "0.3.0",
    "ipfs-http-client": "48.1.1",
    "jest": "^26.6.3",
    "key-did-provider-ed25519": "^1.0.0",
    "multiformats": "3.0.3",
    "node-config-ts": "^3.1.0",
    "tmp-promise": "^2.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "typestub-multihashes": "^0.0.4",
    "unique-names-generator": "^4.3.1"
  },
  "jest": {
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "/node_modules(?!/did-jwt)/"
    ],
    "moduleNameMapper": {
      "multiformats/basics": "multiformats/cjs/src/basics-import.js",
      "multiformats/legacy": "multiformats/cjs/src/legacy.js"
    }
  }
}
